{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@contents/*": [
        "src/contents/*"
      ],
      "@images/*": [
        "src/images/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}